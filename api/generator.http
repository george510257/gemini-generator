### 代码生成器 generator
POST http://localhost:8081/code/generate
Content-Type: application/json

{
  "maven": {
    "groupId": "com.gls.gemini.generator",
    "artifactId": "gemini-generator",
    "version": "0.0.1-SNAPSHOT",
    "name": "Gemini Generator",
    "description": "代码生成器服务",
    "parentVersion": "0.0.1-SNAPSHOT"
  },
  "datasource": {
    "name": "generator",
    "url": "jdbc:mysql://docker-server:3306/gemini_generator?useUnicode=true&characterEncoding=utf-8&useSSL=false&serverTimezone=UTC",
    "username": "root",
    "password": "root",
    "driverClassName": "com.mysql.cj.jdbc.Driver"
  },
  "tablePrefix": "t_"
}

